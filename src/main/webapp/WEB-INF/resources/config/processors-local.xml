<!--
	Description
	Declaration of custom processors for the LDT
-->
<processors xmlns:oxf="http://www.orbeon.com/oxf/processors">
    <processor name="oxf:excel-serializer">
        <class name="nl.architolk.ldt.processors.ExcelSerializer"/>
    </processor>
    <processor name="oxf:word-serializer">
        <class name="nl.architolk.ldt.processors.WordSerializer"/>
    </processor>
	<processor name="oxf:excel-converter">
		<class name="nl.architolk.ldt.processors.ExcelConverter"/>
	</processor>
	<processor name="oxf:xmlfo-processor">
		<class name="nl.architolk.ldt.processors.XMLFOProcessor"/>
	</processor>
	<processor name="oxf:httpclient-processor">
		<class name="nl.architolk.ldt.processors.HttpClientProcessor"/>
	</processor>
	<processor name="oxf:markdown-converter">
		<class name="nl.architolk.ldt.processors.MarkDownConverter"/>
	</processor>
	<processor name="oxf:sparql-parser">
		<class name="nl.architolk.ldt.processors.SparqlParser"/>
	</processor>
	<processor name="oxf:sparql-processor">
		<class name="nl.architolk.ldt.processors.SparqlProcessor"/>
	</processor>
	<processor name="oxf:rdf4j-processor">
		<class name="nl.architolk.ldt.processors.RDF4JProcessor"/>
	</processor>
</processors>
